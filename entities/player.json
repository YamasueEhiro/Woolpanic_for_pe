{
  "minecraft:entity": {
    "format_version": 0.1,
    "component_groups": {
      "on_timer": {
        "minecraft:timer": {
          "time": 1,
          "looping": true,
          "time_down_event": {
            "event": "cheack_time",
            "target": "self"
          }
        }
      },
      "reset": {

      },
      "summon": {
        "minecraft:addrider": {
          "entity_type": "minecraft:donkey"
        },
        "minecraft:is_sheared": {
        },
        "minecraft:type_family": {
        "family": [
          "player","donkey"
        ]
      }
      }
    },
    "components": {
      "minecraft:environment_sensor": [
        {
          "on_environment": {
            "filters": {
              "with_environment_any": [
                "altitude_greater:180",
                "altitude_lss:190"
                
              ]
            },
            "event": "on_timer"
          }
        },
        {
          "on_environment": {
            "filters": {
              "with_environment_any": [
                "altitude_greater:200",
                "altitude_lss:210"
              ]
            },
            "event": "reset"
          }
        }
      ],
      "minecraft:identifier": {
        "id": "minecraft:player"
      },
      "minecraft:type_family": {
        "family": [
          "player"
        ]
      },
      "minecraft:loot": {
        "table": "loot_tables/empty.json"
      },
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 1.9
      },
      "minecraft:movement": {
        "value": 0.1,
        "max": 0.5
      },
      "minecraft:attack": {
        "damage": 1
      },
      "minecraft:player.saturation": {
        "value": 20
      },
      "minecraft:player.exhaustion": {
        "value": 0,
        "max": 4
      },
      "minecraft:player.level": {
        "value": 0,
        "max": 24791
      },
      "minecraft:player.experience": {
        "value": 0,
        "max": 1
      },
      "minecraft:fall_damage": {
        "value": 0.0
      },
      "minecraft:breathable": {
        "totalSupply": 0,
        "suffocateTime": 0,
        "generatesBubbles": false
      },
      "minecraft:rideable": {
        "seat_count": 2,
        "seats": [
          {
            "position": [
              0.0,
              -0.9,
              0.0
            ],
            "min_rider_count": 0,
            "max_rider_count": 1,
            "lock_rider_rotation": 0
          },
          {
            "position": [
              0.0,
              -0.9,
              0.0
            ],
            "min_rider_count": 2,
            "max_rider_count": 2,
            "lock_rider_rotation": 0
          },
          {
            "position": [
              0.0,
              -0,
              0.0
            ],
            "min_rider_count": 2,
            "max_rider_count": 2,
            "lock_rider_rotation": 0
          }
        ]
      },
      "minecraft:inherit_rotation_when_riding": {
        "value": true
      },
      "minecraft:nameable": {
        "alwaysShow": true,
        "allowNameTagRenaming": false
      },
      "minecraft:timer": {
        "time": 1,
        "looping": true,
        "time_down_event": {
          // "event": "summon",
          "target": "self"
        }
      }
    },
    "events": {
      "on_timer": {
        "sequence": [
        {
          "filters": {
              "without_components": "minecraft:timer"
            },
          "add": {
          "component_groups": [
            "on_timer"
          ]
        }}]
      },
      "cheack_time": {
        "sequence": [
          {
            "filters": {
              "with_armor":"golden_helmet",
              "without_components": "minecraft:is_sheared"
            },
            "add": {
              "component_groups": [
                "summon"
              ]
            }
          }
        ]
      },
      "summon": {
        "remove": {},
        "add": {
          "component_groups": [
            "summon"
          ]
        }
      },
      "reset": {
        "remove": {
          "component_groups": [
            "summon",
            "on_timer"
          ]
        },
        "add": {
          "component_groups": [
            "reset"
          ]
        }
      }
    }
  }
}